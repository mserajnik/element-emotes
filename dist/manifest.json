{
  "name": "Element Emotes",
  "description": "A browser extension for custom emote support in Element",
  "version": "1.3.1",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": ["https://app.element.io/*"],
      "js": ["app.js"],
      "css": ["app.css"]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
